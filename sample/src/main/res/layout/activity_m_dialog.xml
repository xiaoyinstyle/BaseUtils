<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.jskingen.baseutils.yin.jskingen.baseutils.utils.mDialogActivity">

    <Button
        android:id="@+id/loadingDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LoadingDialog"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/loading_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="自动消失 ok"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/loading_error"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="自动消失 error"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/loading_waring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="自动消失 waring"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/loading_other"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="自动消失 自定义"
        android:textAllCaps="false" />
</LinearLayout>
